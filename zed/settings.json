{
  "telemetry": {
    "metrics": false
  },
  "features": {
    "edit_prediction_provider": "none"
  },
  "theme": "Nord",
  "agent": {
    "always_allow_tool_actions": true,
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-sonnet-4"
    },
    "version": "2"
  },
  "auto_install_extensions": {
    "dockerfile": true,
    "docker-compose": true,
    "html": false,
    "justfile": true,
    "nord": true,
    "sql": true,
    "toml": true,
    "xml": true,
    "zig": true
  },
  "base_keymap": "VSCode",
  "vim_mode": true,
  "vim": {
    "use_system_clipboard": "never"
  },
  "show_wrap_guides": false,
  "soft_wrap": "none",
  "confirm_quit": true,
  "ui_font_size": 16,
  "buffer_font_size": 14,
  "buffer_font_features": {
    "calt": false
  },
  "git": {
    "git_gutter": "hide",
    "inline_blame": {
      "enabled": false
    }
  },
  "languages": {
    "YAML": { "prettier": { "allowed": false } }
  },
  "ssh_connections": [
    {
      "host": "jlinux01",
      "username": "jcalabro",
      "projects": [
        {
          "paths": [
            "/home/jcalabro/go/src/github.com/bluesky-social/bsky-infra"
          ]
        },
        {
          "paths": [
            "/home/jcalabro/go/src/github.com/bluesky-social/tango"
          ]
        }
      ]
    }
  ]
}
