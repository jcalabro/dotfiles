{
  "features": {
    "edit_prediction_provider": "none"
  },
  "theme": "Nord",
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-7-sonnet-latest"
    },
    "version": "2"
  },
  "auto_install_extensions": {
    "dockerfile": true,
    "docker-compose": true,
    "html": false,
    "justfile": true,
    "nord": true,
    "sql": true,
    "toml": true,
    "xml": true,
    "zig": true
  },
  "base_keymap": "VSCode",
  "vim_mode": true,
  "vim": {
    "use_system_clipboard": "never"
  },
  "confirm_quit": true,
  "ui_font_size": 16,
  "buffer_font_size": 14,
  "buffer_font_features": {
    "calt": false
  },
  "git": {
    "git_gutter": "hide",
    "inline_blame": {
      "enabled": false
    }
  },
  "ssh_connections": [
    {
      "host": "jlinux01",
      "username": "jcalabro",
      "projects": [
        {
          "paths": [
            "~/go/src/github.com/ChessCom/ceac-services"
          ]
        },
        {
          "paths": [
            "~/go/src/github.com/ChessCom/explanation-engine",
            "~/go/src/github.com/ChessCom/explanation-engine-interface",
            "~/go/src/github.com/ChessCom/ceac-services"
          ]
        },
        {
          "paths": [
            "~/go/src/github.com/jcalabro/airbus"
          ]
        },
        {
          "paths": [
            "~/go/src/github.com/jcalabro/reflex"
          ]
        }
      ]
    }
  ]
}
