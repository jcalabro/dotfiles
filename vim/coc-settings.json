        "rust": {
            "command": "rust-analyzer",
            "filetypes": ["rust"],
            "rootPatterns": ["Cargo.toml"]
        }
    },
    "suggest.enablePreselect": true,
    "suggest.noselect": false,
    "zig.enabled": true,
    "zig.startUpMessage": true,
    "zig.path": "/home/jcalabro/.bin/zls",
    "zig.debugLog": false,
    "zig.zls.path": "/home/jcalabro/.config/coc/extensions/coc-zls-data/zls_install/zls"
